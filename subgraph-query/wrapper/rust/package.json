{
  "name": "subgraph-query-rs",
  "description": "Subgraph query demo rust wrapper",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "build": "polywrap build",
    "codegen": "polywrap codegen",
    "test:env:up": "polywrap infra up --modules=eth-ens-ipfs",
    "test:env:down": "polywrap infra down --modules=eth-ens-ipfs",
    "deploy": "polywrap deploy",
    "test": "yarn test:e2e && yarn test:workflow",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "polywrap app codegen -m ./src/__tests__/types/polywrap.app.yaml -g ./src/__tests__/types/wrap",
    "test:workflow": "yarn test:env:up && sleep 5 && polywrap run ./workflows/e2e.yaml && yarn test:env:down"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "jest": "26.6.3",
    "polywrap": "0.2.0",
    "ts-jest": "26.5.4",
    "typescript": "4.0.7"
  }
}
